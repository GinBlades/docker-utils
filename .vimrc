syntax on
filetype plugin indent on

set number
set shortmess=at
set incsearch
set hlsearch
set gdefault
set backspace=indent,eol,start
set autoindent
set shiftround
set shiftwidth=4
set softtabstop=4
set expandtab
set wildmenu
set showcmd
map ; :
inoremap jk <esc>
nmap <C-J> :m+1<CR>
nmap <C-K> :m-2<CR>
